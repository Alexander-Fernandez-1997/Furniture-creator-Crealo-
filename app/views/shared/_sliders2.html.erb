<div class="container">
<div class="row">
<h2>List of textures</h2>

<div class="container">
<div class = "row multi-columns-row cajita">
<% Texture.all.each do |texture|  %>
<div class="col-sm-6 col-md-3 col-lg-3">
<img class="imgsize" src= <%= texture.url %>  alt=" <% texture.url %>">
</div>
<h2> <%= texture.name %> </h2>
</div>
<% end  %>
</div>
</div>
</div>